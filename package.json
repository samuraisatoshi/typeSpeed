{
  "name": "typespeed",
  "version": "1.0.0",
  "description": "Local code typing practice application like MonkeyType for programmers",
  "main": "dist/index.js",
  "scripts": {
    "dev": "tsx watch src/infrastructure/web/server.ts",
    "build": "tsc",
    "start": "node dist/infrastructure/web/server.js",
    "test": "jest",
    "lint": "eslint src/**/*.ts",
    "format": "prettier --write src/**/*.ts",
    "domain:validate": "tsx scripts/validate-domain-map.ts",
    "clean": "rm -rf dist"
  },
  "keywords": [
    "typing",
    "code",
    "practice",
    "monkeytype",
    "programming"
  ],
  "author": "TypeSpeed Team",
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "express": "^5.1.0",
    "express-rate-limit": "^8.2.1",
    "express-validator": "^7.3.0",
    "helmet": "^8.1.0",
    "multer": "^2.0.2",
    "prismjs": "^1.30.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.5",
    "@types/express-rate-limit": "^5.1.3",
    "@types/jest": "^30.0.0",
    "@types/multer": "^2.0.0",
    "@types/node": "^24.9.2",
    "@typescript-eslint/eslint-plugin": "^8.46.2",
    "@typescript-eslint/parser": "^8.46.2",
    "eslint": "^9.39.0",
    "jest": "^30.2.0",
    "prettier": "^3.6.2",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3"
  }
}
